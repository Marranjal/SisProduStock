	<queryString>
		<![CDATA[SELECT
     venda.`dataHoraVenda` AS venda_dataHoraVenda,
     venda.`valorTotal` AS venda_valorTotal,
     itensvenda.`quantidade` AS itensvenda_quantidade,
     itensvenda.`total` AS itensvenda_total,
     produto.`designacao1` AS produto_designacao1,
     caixa.`nomeCaixa` AS caixa_nomeCaixa,
     cliente.`nomeCliente` AS cliente_nomeCliente,
     cliente.`endereco` AS cliente_endereco,
     cliente.`nif` AS cliente_nif,
     produtostock.`quantidade` AS produtostock_quantidade,
     venda.`idvenda` AS venda_idvenda,
     pagamentoforma.`designacaoForma` AS pagamentoforma_designacaoForma
FROM
     `venda` venda INNER JOIN `itensvenda` itensvenda ON venda.`idvenda` = itensvenda.`venda_idvenda`
     INNER JOIN `produtostock` produtostock ON itensvenda.`produtoStock_idprodutoStock` = produtostock.`idprodutoStock`
     INNER JOIN `produto` produto ON produtostock.`produto_idproduto` = produto.`idproduto`
     INNER JOIN `caixa` caixa ON venda.`caixa_idcaixa` = caixa.`idcaixa`
     INNER JOIN `cliente` cliente ON venda.`cliente_idcliente` = cliente.`idcliente`
     INNER JOIN `pagamentoforma` pagamentoforma ON venda.`pagamentoForma_idpagamentoForma` = pagamentoforma.`idpagamentoForma`]]>
	</queryString>
